ssName=\"page-heading\">Welcome, {username}!</h1>\n          <button className=\"button login__submit logout_button\" type=\"submit\"  onClick={handleLogout}>\n          <span className=\"button__text\">Logout</span>\n          <i className=\"button__icon fas fa-chevron-right\"></i>\n        </button>\n        <div  className=\"tableStyle\">\n          <table>\n            <thead>\n              <tr style={{background:\"grey\"}}>\n                <th>Name</th>\n                <th>Email</th>\n                <th>Message</th>\n              </tr>\n            </thead>\n            <tbody>\n              {formSubmissions.map((submission, index) => (\n                <tr key={index}>\n                  <td>{submission.name}</td>\n                  <td>{submission.email}</td>\n                  <td>{submission.message}</td>\n                </tr>\n              ))}\n            </tbody>\n          </table>\n          </div>\n        </div>\n      ) : (\n        <div>\n           <h1 className=\"page-heading\">Admin</h1>\n          <div className=\"container-contact\">\n            <div className=\"screen1\">\n              <div className=\"screen__content\">\n                <form className=\"login1\">\n                  <div className=\"login__field\">\n                    <i className=\"login__icon fas fa-user\"></i>\n                    <input\n                      className=\"login__input\"\n                      placeholder=\"Username\"\n                      type=\"text\"\n                      value={username}\n                      onChange={(e) => setUsername(e.target.value)}\n                    />\n                  </div>\n                  <div className=\"login__field\">\n                    <i className=\"login__icon fas fa-lock\"></i>\n                    <input\n                      className=\"login__input\"\n                      placeholder=\"Password\"\n                      type=\"password\"\n                      value={password}\n              